<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <stroke android:width="2dp"
                android:color="#35A735"/>
        </shape>
    </item>

    <item android:bottom="2dp"
        android:left="2dp"
        android:right="2dp"
        android:top="2dp">
        <shape android:shape="oval">
            <stroke android:color="#7D7D7D"
                android:width="2dp"/>
        </shape>
    </item>

    <item
        android:bottom="4dp"
        android:top="4dp"
        android:left="4dp"
        android:right="4dp">
        <shape android:shape="oval">
            <solid android:color="#35A735"/>
        </shape>
    </item>

</layer-list>